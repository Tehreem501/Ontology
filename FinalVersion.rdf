    <owl:Ontology rdf:about="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign">
        <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-01</dc:created>
        <dc:creator>Tehreem/UOS</dc:creator>
        <dc:description xml:lang="en">An ontology for representing UI/UX design elements, accessibility requirements, design principles, and recommendations for interface evaluation.</dc:description>
        <dc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-12-12</dc:modified>
        <dc:title xml:lang="en">UI/UX Design Ontology</dc:title>
        <rdfs:comment xml:lang="en">This ontology models UI/UX design components following Atomic Design principles, integrates accessibility standards (WCAG), and supports AI-powered design recommendation systems with hallucination detection.</rdfs:comment>
        <owl:versionInfo>1.0.0</owl:versionInfo>
    </owl:Ontology>
    



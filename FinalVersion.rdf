        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/NCIT_C210922"/>
        <rdfs:comment>Component is affected by issue</rdfs:comment>
        <rdfs:label>affected by issue</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#affectsComponent -->


    <!-- http://schema.org/color -->

    <owl:ObjectProperty rdf:about="http://schema.org/color">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#colorOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#DesignPrinciples"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#InteractionState"/>
                    <rdf:Description rdf:about="https://uxiverse.com/ontology/UIElement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://purl.dsmz.de/schema/Color"/>
        <rdfs:comment>Element, principle, or state has color property</rdfs:comment>
        <rdfs:label>color</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/isPartOf -->

    <owl:ObjectProperty rdf:about="http://schema.org/isPartOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#contains"/>
        <rdfs:domain rdf:resource="https://uxiverse.com/ontology/UIElement"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#UISystem"/>
                    <rdf:Description rdf:about="https://uxiverse.com/ontology/UIElement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Component is part of system or parent component</rdfs:comment>
        <rdfs:label>is part of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/parentOrganization -->

    <owl:ObjectProperty rdf:about="http://schema.org/parentOrganization">
        <owl:inverseOf rdf:resource="http://schema.org/subOrganization"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://uxiverse.com/ontology/UIElement"/>
        <rdfs:range rdf:resource="https://uxiverse.com/ontology/UIElement"/>
        <rdfs:comment>Relationship to a parent organization. Transitive: if A has parent B and B has parent C, then A has parent C.</rdfs:comment>
        <rdfs:label>parent organization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/subOrganization -->

    <owl:ObjectProperty rdf:about="http://schema.org/subOrganization">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://uxiverse.com/ontology/UIElement"/>
        <rdfs:range rdf:resource="https://uxiverse.com/ontology/UIElement"/>
        <rdfs:comment>Relationship to a subsidiary organization. Transitive: if A has sub-org B and B has sub-org C, then A has sub-org C.</rdfs:comment>
        <rdfs:label>sub organization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#addressedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#addressedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#basedOnIssue"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/NCIT_C210922"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/NCIT_C25197"/>
        <rdfs:comment>Issue addressed by recommendation</rdfs:comment>
        <rdfs:label>addressed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#affectedByIssue -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pmyls/ontologies/2025/10/uiuxdesign#affectedByIssue">
